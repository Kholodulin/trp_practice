   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "time = np.linspace(0, 10, 100)\n",
    "data = np.sin(time) + np.random.normal(scale=0.1, size=100)\n",
    "\n",
    "plt.plot(time, data, label='Измерения')\n",
    "plt.title('График')\n",
    "plt.xlabel('Время')\n",
    "plt.ylabel('Значения')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  }
